    "jest": "^29.7.0",
    "jscpd": "^4.0.5",
    "prettier": "3.5.3",
    "stylelint": "^16.18.0",
    "stylelint-config-standard": "^38.0.0",
    "ts-jest": "^29.3.2",
    "ts-node": "^10.9.2",
